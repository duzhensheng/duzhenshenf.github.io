
public class Solution {
    public int Fibonacci(int n) {
    if (n == 0 || n == 1) {
            return n;
        }
        int first = 0;
        int second = 1;
        int ans = 0;
        for (int i = 2; i <= n; i++) {
            ans = first + second;
            first = second;
            second = ans;
        }
        return ans;
    }
}
